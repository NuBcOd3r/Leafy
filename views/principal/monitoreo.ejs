<!DOCTYPE html>
<html lang="es">

<%- include('../layouts/headPrincipal') %>

<body>
    <%- include('../layouts/headerPrincipal') %> 
    <%- include('../layouts/navbar') %>

    <main class="main-content">
        <div class="container-fluid mt-4">
            <div class="row g-4 align-items-start">

                <div class="col-md-7">
                    <div id="carouselExample" class="carousel slide shadow rounded" data-bs-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="" class="d-block w-100 rounded" alt="Foto 1">
                            </div>
                            <div class="carousel-item">
                                <img src="" class="d-block w-100 rounded" alt="Foto 2">
                            </div>
                            <div class="carousel-item">
                                <img src="" class="d-block w-100 rounded" alt="Foto 3">
                            </div>
                        </div>

                        <button class="carousel-control-prev" type="button"
                            data-bs-target="#carouselExample" data-bs-slide="prev">
                            <span class="carousel-control-prev-icon"></span>
                        </button>

                        <button class="carousel-control-next" type="button"
                            data-bs-target="#carouselExample" data-bs-slide="next">
                            <span class="carousel-control-next-icon"></span>
                        </button>
                    </div>
                </div>

                <div class="col-md-5">
                    <div id="lecturasContainer" class="row g-3">
                    </div>
                </div>

            </div>
        </div>
    </main>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="/js/monitoreo.js"></script>
